{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    // This rule enforces consistent use of trailing commas in object and array literals.
    "comma-dangle": 0
  }
}